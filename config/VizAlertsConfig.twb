        <column caption='default_notify_subscriber_on_failure' datatype='boolean' folder-name='0 - General Default Settings' name='[Parameter 6]' param-domain-type='list' role='measure' type='nominal' value='true'>
          <calculation class='tableau' formula='true' />
          <desc>
            <formatted-text>
              <run>System-wide default for whether or not a Subscriber should be emailed when their VizAlert fails to  process successfully, for whatever reason. Recommend &quot;true&quot;.

For more information, or to override this setting dynamically, see the</run>
              <run bold='true'> notify_subscriber_on_failure </run>
              <run>calculation.</run>
            </formatted-text>
          </desc>
        </column>
        <column caption='default_action_enabled_email' datatype='integer' folder-name='1 - Email Action Default Settings' name='[Parameter 7]' param-domain-type='list' role='measure' type='quantitative' value='1'>
          <calculation class='tableau' formula='1' />
